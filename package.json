{
  "name": "ai-assistant-orchestrator",
  "version": "1.0.0",
  "description": "SMS-controlled AI agent orchestrator with LLM-planned automation and secure execution",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "start": "tsx src/index.ts",
    "build": "tsc",
    "cli": "tsx src/cli.ts",
    "db:generate": "prisma generate --schema=src/store/prisma/schema.prisma",
    "db:push": "prisma db push --schema=src/store/prisma/schema.prisma",
    "db:studio": "prisma studio --schema=src/store/prisma/schema.prisma"
  },
  "bin": {
    "assistant": "./bin/assistant.sh"
  },
  "keywords": [
    "ai",
    "agent",
    "orchestrator",
    "sms",
    "twilio",
    "claude",
    "automation"
  ],
  "type": "module",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.39.0",
    "@prisma/client": "^6.2.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "node-cron": "^3.0.3",
    "twilio": "^5.4.3",
    "uuid": "^11.0.5",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/node": "^22.12.0",
    "@types/node-cron": "^3.0.11",
    "@types/uuid": "^10.0.0",
    "prisma": "^6.2.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
